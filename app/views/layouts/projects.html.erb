<% content_for :head do %>
    <%= javascript_pack_tag 'dashboard' %>
<% end %>

<% content_for :nav do %>
    <%= render 'dashboard_navigation' %>
    <% if content_for? :banner then %>
        <%= yield :banner %>
    <% end %>
<% end %>

<% content_for :content do %>
    <div class="min-container clearfix" id="main">
        <app-root></app-root>
    </div>
<% end %>

<% content_for :footer do %>
    <footer><p>Copyright (c) Harry Felton 2017-2018. See <a href="#">imprint file</a> for license information.</p></footer>
<% end %>

<%= render file: 'layouts/application' %>
